{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "camelcase": "off",
        "jsx-a11y/label-has-associated-control": "off",
        "linebreak-style": "off",
        "max-len": [
            "error",
            {
                "code": 100,
                "ignoreStrings": true
            }
        ],
        "react/forbid-prop-types": "off",
        "react/jsx-props-no-spreading": "off",
        "react/require-default-props": "off",
        "semi": "off"
    }
}
